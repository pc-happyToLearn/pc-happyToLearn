/* pages/demo02/demo02.wxss */
.table {
    border: 0px solid darkgray;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 106rpx;
    align-items: center;
}

.td {
    width: 40%;
    justify-content: center;
    text-align: center;
}
.bg-w {
    background: snow;
}
.bg-g {
    background: #eeeeee;
}

.th {
    width: 40%;
    justify-content: center;
    background: #1296db;
    color: #fff;
    font-weight: bold;
    font-size: 13px;
    display: flex;
    height: 92rpx;
    align-items: center;
}
.first{
    font-size:12px;
}
.pageControl{
    height: 1.5rem;
    width: 100%;
}
.up{
    height: 1.5rem;
    width: 20%;
    margin: 2% 5% 0% 10.5%;
    padding: 1%;
    text-align: center;
    color: #1296db;
    border: 1px solid #1296db;
    border-radius: 20px;
    float: left;
}
.upSelected{
    height: 1.5rem;
    width: 20%;
    margin: 2% 5% 0% 10.5%;
    padding: 1%;
    text-align: center;
    color: #666666;
    border: 1px solid #666666;
    border-radius: 20px;
    float: left;
}
.pageBottomTxt{
    float: left;
    font-size: 12px;
    margin: 4% 0% 0% 0%;
}
.down{
    height: 1.5rem;
    width: 20%;
    margin: 2% 5% 0% 5%;
    padding: 1%;
    text-align: center;
    color: #1296db;
    border: 1px solid #1296db;
    border-radius: 20px;
    float: left;
}
.downSelected{
    height: 1.5rem;
    width: 20%;
    margin: 2% 5% 0% 5%;
    padding: 1%;
    text-align: center;
    color: #666;
    border: 1px solid #666;
    border-radius: 20px;
    float: left;
}
.topInput{
    margin: 23rpx;
    float: left;
    width: 24%;
    border: 1px solid #1296db;
    font-size: 10px;
    padding: 10rpx;
    box-shadow: 3px 3px 7px #888888;
}
.topTxt{
    position: relative;
    top: 30rpx;
    font-size: 13px;
    float: left;
    margin: 2px;
}
#checkButton{
    width: 159rpx;
    font-size: 14px;
    font-weight: 400;
    color: #fff;
    float: left;
    position: relative;
    top: 22rpx;
    background: #1296db;
    box-shadow: 3px 3px 7px #888888;
}
.key-box{
    width: 40%;
    float: left;
    margin: 22rpx;
    padding: 4rpx;
    border: 1px solid #1296db;
    position: relative;
    top: -11rpx;
    box-shadow: 3px 3px 7px #888888;
}
#pull{
    position: relative;
    top: 0rpx;
    float: right;
    width: 56rpx;
    height: 55rpx;
    border-left: 1px solid #8a8a8a;
}
.topInput3{
    padding: 4rpx 17rpx
}
.popup{
    z-index: 2;
    position: absolute;
    width: 100%;
    height: 100%;
    background: rgba(196,196,196,0.9);
    top: 0;
    left: 0;
}
.popupPage{
    background: #fff;
    width: 92%;
    height: 50%;
    position: absolute;
    top: 24%;
    left: 4%;
    border-radius: 7%;
}
.drop{
    width: 40rpx;
    height: 40rpx;
    float: right;
    margin: 31rpx;
}
.popupTitle{
    width: 100%;
    height: 13%;
    border-bottom: 1px solid #1296db;
}
.popupTitleTxt{
    float: left;
    height: 83%;
    width: 84%;
    text-align: center;
    font-size: 39rpx;
    color: #8a8a8a;
    position: relative;
    top: 18rpx;
    left: 35rpx;
}
.popupPageContent{
    width: 90%;
    height: 34%;
    position: relative;
    top: 31%;
    left: 6%;
    font-size: 40rpx;
    color: #8a8a8a;
}
.popupProduct{
    position: relative;
    left: -3%;
    width: 100%;
    margin: 10rpx 5rpx;
    padding: 7rpx;
    border-bottom: 1px solid #8a8a8a;
}
.popupLeftTxt{
    width: 25%;
    position: relative;
    padding: 18rpx;
    border-right: 1px solid #8a8a8a;
}
.popupRightInput{
    float: left;
    position: absolute;
    right: 8%;
    top: 26%;
    width: 52%;
    font-size: 36rpx;
    text-align: center;
    border-bottom: 1.2px solid #000000;
}
.popupCodeTxt{
    float: left;
    position: absolute;
    right: 8%;
    top: 26%;
    width: 52%;
    text-align: center;
}
.popupButton{
    width: 11%;
    font-size: 20px;
    color: white;
    position: absolute;
    padding: 8rpx 26rpx;
    border-radius: 9px;
    background: #1296db;
    bottom: 27.5%;
    right: 7%;
}